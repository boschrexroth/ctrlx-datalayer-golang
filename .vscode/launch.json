  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Datalayer",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/cmd/datalayer",
      "cwd": "${workspaceRoot}",
      "args": [
        "-client",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2069",
        "-provider",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2070",
      ],
      "showLog": true,
    },
    {
      "name": "Launch Benchmark",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/cmd/benchmark",
      "cwd": "${workspaceRoot}",
      "args": [
        "-client",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2069",
        "-provider",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2070",
      ],
      "showLog": true,
    }
  ]
}