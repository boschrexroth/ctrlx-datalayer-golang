  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Prebuild Datalayer",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceRoot}/build/public/linux_amd64/datalayer",
      "cwd": "${workspaceRoot}/build/public/linux_amd64",
      "args": [
        "-client",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2069",
        "-provider",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2070",
      ],
      "showLog": true,
      "env": {
        "LD_LIBRARY_PATH": "${LD_LIBRARY_PATH}:${workspaceRoot}/deps/public/bin/comm.datalayer/linux-gcc-x64/release:${workspaceRoot}/deps/public/bin/zmq/linux-gcc-x64/release"
      }
    },
    {
      "name": "Launch Prebuild Benchmark",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceRoot}/build/public/linux_amd64/benchmark",
      "cwd": "${workspaceRoot}/build/public/linux_amd64",
      "args": [
        "-client",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2069",
        "-provider",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2070",
      ],
      "showLog": true,
      "env": {
        "LD_LIBRARY_PATH": "${LD_LIBRARY_PATH}:${workspaceRoot}/deps/public/bin/comm.datalayer/linux-gcc-x64/release:${workspaceRoot}/deps/public/bin/zmq/linux-gcc-x64/release"
      }
    },
    {
      "name": "Launch Datalayer",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/cmd/datalayer",
      "cwd": "${workspaceRoot}",
      "args": [
        "-client",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2069",
        "-provider",
        "tcp://boschrexroth:boschrexroth@192.168.1.1:2070",
